namespace :app do 
  desc "Seeding steam server login tokens" 
  task :seed_gslt_csgo_list, [] => :environment do |t, args|
    gslt_csgo_list.split("\n").each do |token|
      unless ApiDeploy::SteamServerLoginToken.exists?(token: token)
        model = ApiDeploy::SteamServerLoginToken.create({
          :app_id  => 730,
          :token   => token,
          :in_use  => false
        })
        puts "===== Adding token #{token}"
      end
    end
  end
end